<template>
    <view class="flex-col">
        <block wx:if="{{chatItem.type!=='custom'}}">
            <text wx:if="{{chatItem.showTime=='true'}}" class="chat-list-time-style">{{chatItem.time}}</text>
            <view style="width: 100%;display: flex;flex-direction: {{chatItem.isMy=='true'?'row-reverse':'row'}};margin-bottom: {{index===length-1?150:20}}rpx;margin-top: 20rpx;">
                <image class="avatar"
                    src="{{chatItem.headUrl}}"/>
                <image wx:if="{{chatItem.type==='text'||chatItem.type==='voice'}}" class="chat-list-arrow-style"
                    src="../image/chat/popu_{{chatItem.isMy=='true'?'blue':'white'}}.png"/>

                <block wx:if="{{chatItem.type==='text'}}">
                    <view wx:if="{{chatItem.type==='text'}}" class="{{chatItem.isMy=='true'?'isMyWordStyle':'isOtherWordStyle'}}"
                        style="border-radius: 10rpx;padding: 20rpx;font-size: 30rpx;max-width: 60%;"
                        catchtap="chatTextItemClickEvent"
                        data-index="{{index}}">{{chatItem.content}}</view>
                </block>

                <block wx:if="{{chatItem.type==='image'}}">
                    <image wx:if="{{chatItem.type==='image'}}" class="chat-list-pic-style" src="{{chatItem.content}}"
                        mode="aspectFill" catchtap="imageClickEvent" data-url="{{chatItem.content}}"/>
                </block>

                <block wx:if="{{chatItem.type==='voice'}}">
                    <view style="width: {{(chatItem.voiceDuration-1)*0.6+10}}%;border-radius: 10rpx;padding: 20rpx;font-size: 30rpx;max-width: 60%;display: flex;justify-content: {{isMy=='true'?'flex-end':'flex-start'}}"
                        class="{{chatItem.isMy=='true'?'isMyWordStyle':'isOtherWordStyle'}}"
                        catchtap="chatVoiceItemClickEvent"
                        data-voice-path="{{chatItem.content}}"
                        data-voice-duration="{{chatItem.voiceDuration}}"
                        data-is-my="{{chatItem.isMy=='true'}}"
                        data-index="{{index}}">
                        <image wx:if="{{chatItem.isMy=='true'}}" class="voice-play-size"
                            src="{{chatItem.isPlaying=='true'?'data:image/gif;base64,':'./image/chat/voice/voice_mine.png'}}" />
                        <image wx:else class="voice-play-size"
                            src="{{chatItem.isPlaying=='true'?'data:image/gif;base64,':'./image/chat/voice/voice_other.png'}}" />
                    </view>
                    <view class="voice-duration-style">{{chatItem.voiceDuration}}"</view>
                </block>
                <block wx:if="{{chatItem.isMy=='true'}}">
                        <view class="sendStatusStyle" wx:if="{{chatItem.sendStatus!=='success'}}">
                            <block wx:if="{{chatItem.sendStatus==='sending'}}">
                                <image class="chat-loading-common" src="data:image/gif;base64,"/>
                            </block>
                            <block wx:if="{{chatItem.sendStatus==='fail'}}">
                                <image src="../image/chat/send_fail.png" class="chat-loading-common" catchtap="resendMsgEvent" data-resend-index="{{index}}" />
                            </block>
                        </view>
                </block>
            </view>
        </block>
        <block wx:else>
            <text class="chat-list-time-style">{{chatItem.content}}</text>
        </block>
    </view>
</template>

<script>
const ITEM_EVENT = {
    TEXT_CLICK:'chatTextItemClickEvent',
    IMAGE_CLICK:'imageClickEvent',
    VOICE_CLOCK:'chatVoiceItemClickEvent',
    RESEND_MSG:'resendMsgEvent'
};
import wepy from 'wepy';
export default class ChatItem extends wepy.component{
    props = {
        chatItem:{
            type:Object,
            default:{
                isMy:0,
                avatar:'',
                showTime:0,
                time:'',
                content:'',
                type:'',//text,voice,image,costom
                voiceDuration:0,
                isPlaying:0,
                sendStatus:0,//success,fail,sending
            },
        },
        index:{
            type:Number,
            default:0.
        },
        length:{
            type:Number,
            default:0.
        }
    }
    methods = {
        chatTextItemClickEvent(e){
            this.$emit(ITEM_EVENT.TEXT_CLICK, e);
        },
        imageClickEvent(e){
            this.$emit(ITEM_EVENT.IMAGE_CLICK, e);
        },
        chatVoiceItemClickEvent(e){
            this.$emit(ITEM_EVENT.VOICE_CLOCK, e);
        },
        resendMsgEvent(e){
            this.$emit(ITEM_EVENT.RESEND_MSG, e);
        }
    }

}
</script>

<style>
.flex-col{
    width: 100%;display: flex;flex-direction: column;align-items: center;
}

.chat-list-time-style {
    margin-top: 30rpx;
    background-color: #cccccc;
    color: white;
    padding: 8rpx 10rpx 6rpx 10rpx;
    border-radius: 6rpx;
    width: auto;
    font-size: 22rpx;
    line-height: 22rpx;
    text-align: center;
}

.avatar{
    width: 70rpx;height: 70rpx;border-radius: 50%;margin-left: 15rpx;margin-right: 15rpx;
}

.isMyWordStyle {
    background-color: #8fcbf7;
    color: #213d52;
    margin-right: -1rpx;
    margin-left: 14rpx;
    word-wrap: break-word;
}

.isOtherWordStyle {
    background-color: white;
    color: #333333;
    margin-right: 14rpx;
    margin-left: -1rpx;
    word-wrap: break-word;
}

.chat-list-pic-style {
    border-radius: 10rpx;
    margin-left: 5rpx;
    width: 150rpx;
    height: 280rpx;
}

.voice-duration-style {
    color: #666666;
    font-size: 26rpx;
    align-self: center;
    margin-top: 3rpx;
    margin-left: 5rpx;
}

.chat-list-arrow-style {
    width: 11rpx;
    height: 20rpx;
    margin-top: 25rpx
}

.voice-play-size{
    width:22rpx;
    height:32rpx;
}

.sendStatusStyle{
    height:100%;
    display: flex;
    align-content: center;
    justify-content: center;
    flex-direction: column;
}

.chat-loading-common{
    width:40rpx;
    height:40rpx;
}
</style>
